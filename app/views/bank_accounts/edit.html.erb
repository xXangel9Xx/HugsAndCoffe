<div>
 <%= render partial: './nav', layout: './nav' %>
 <%= render partial: './nav_desktop', layout: './nav_desktop'%>


 <div class="container-text-create-account-bank">
  <h1 class="create-account-bank">Crear cuenta</h1>
  </div>

  <div class="container-text-information-accountBank">
  <h1 style="font-size:x-large;">Informacion de la cuenta</h1>
  <p>Crea una cuenta donde las personas
  pueden agracerte.</p>
  </div>
  <!--<%= render partial: 'form_bank', layout:'form_bank' %>-->

  <div class="container-create-new-bankAccount">
  <div class="container-form-bankAccount">
  <%=form_with(url:bank_account_path(User.find_by(id: current_user.id).perfil.id, @bank_account), model:@bank_account, local:true) do |f| %>

  <div>
  <br>
  <%=f.label :entidad%>
  <%=f.text_field :entidad%>
  <br>
  </div>

  <div>
  <br>
  <%=f.label :email%>
  <%=f.text_field :email%>
  <br>
  </div>
  <div>
  <br>
  <%=f.label :cuenta%>
  <%=f.number_field :account%>
  <br>
  </div>
  <div>
  <br>
  <%=f.label :identificacion%>
  <%=f.number_field :identification%>
  <br>
  </div>
  <div>
  <br>
  <%=f.label :tipo_de_cuenta%>
  <%=f.text_field :typeAccount%>
  <br>
  <div>

  <div class="container-save-form-bank">
    <%=f.submit 'Guardar', :class =>'save'%>
  </div>
<% end %>
  </div>
</div>
</div>